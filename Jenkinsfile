pipeline{
	agent any
	
	
	stages {
		stage ('Checkot stage'){
			steps {
				 echo 'This is checkout stage'
			}
		
		}
	
		stage ('Test stage'){
			steps {
				 echo 'this is  test stage'
			
			}

		}
		
		stage ('Build stage'){
			steps {
				 echo 'this is build stage'
			}
		
		}
		stage ('Push to docker') {
			steps {
				 echo 'deployed to docker'
			
			}
		
		
		}
	
	}

}
